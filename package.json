{
  "name": "@vue-ui-kit/ant",
  "version": "1.0.0",
  "description": "Vue3 UI Kit based on Ant Design",
  "main": "index.js",
  "scripts": {
    "dev": "vue-tsc --noEmit --skipLibCheck & vite",
    "build": "vite build",
    "prepare": "husky",
    "lint:prettier": "prettier --write  \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\""
  },
  "keywords": [
    "vue3",
    "ui kit",
    "ant design",
    "ant",
    "antdv"
  ],
  "author": {
    "name": "adoin",
    "email": "adoin@qq.com"
  },
  "dependencies": {
    "vue": "^3.4.38"
  },
  "peerDependencies": {
    "@ant-design/icons-vue": ">=7.0.0",
    "ant-design-vue": ">=4.0.0",
    "vue": ">=3.2.0"
  },
  "files": [
    "dist",
    "packages",
    "index.ts"
  ],
  "license": "GPL",
  "engines": {
    "node": ">=16"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.1",
    "@commitlint/config-conventional": "^19.4.1",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "husky": "^9.1.5",
    "lint-staged": "^15.2.9",
    "prettier": "^3.3.3",
    "sass": "^1.77.8",
    "unplugin-vue-setup-extend-plus": "^1.0.1",
    "vite": "^5.4.2",
    "vite-plugin-css-injected-by-js": "^3.5.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": [
      "oxlint --fix",
      "prettier --write"
    ],
    "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
      "prettier --write--parser json"
    ],
    "package.json": [
      "prettier --write"
    ],
    "*.vue": [
      "oxlint --fix",
      "prettier --write"
    ],
    "*.{scss,less,styl,html}": [
      "oxlint --fix",
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  }
}
